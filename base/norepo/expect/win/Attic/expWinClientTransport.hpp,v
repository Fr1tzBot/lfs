a53 14
};

class ClientConio : public ClientTransport
{
public:
    ClientConio(const char *name, CMclQueue<Message *> &_mQ);
    ~ClientConio();
    virtual void Write(Message *);
private:
    CMclQueue<Message *> &mQ;
    HANDLE hConOut;
    HANDLE hConIn;
    HANDLE hOriginalConOut;
    CMclThread *readThread;
@


