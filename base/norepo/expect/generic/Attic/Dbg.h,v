#ifndef TCL_NEWEXTERN
#   ifdef __cplusplus
#	define OLDEXTERN    extern "C" TCL_STORAGE_CLASS
#   else
#	define OLDEXTERN    extern TCL_STORAGE_CLASS
#   endif
#   undef EXTERN
#   define EXTERN(a) OLDEXTERN a
#endif


#undef TCL_STORAGE_CLASS
#define TCL_STORAGE_CLASS DLLEXPORT


