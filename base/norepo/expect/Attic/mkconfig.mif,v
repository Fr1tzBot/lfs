1.1.2.1
log
@New top-root makefile under windows.
@
text
@a0 39
#==============================================================================
#
#  Edit for your local configuration.  This should be the only file
#  that needs editing to build [Tcl/Tk].
#
#==============================================================================
# RCS: @@(#) $Id: mkconfig.mif,v 1.1.2.2 2001/09/15 01:56:54 davygrvy Exp $
#==============================================================================


!error Please set the user defaults in this file before using this build system.


#==============================================
# CVS info
#==============================================
CVSCMD		= c:\dev\wincvs\cvs.exe
CVS_RSH		= c:\cygwin\bin\ssh.exe

#==============================================
# Where should we install to ?
#==============================================
INSTALLDIR	= c:\program files\tcl

#==============================================
# Toolset information
#==============================================

#### Microsoft Visual C++
TOOLSETvc32	= c:\dev\devstudio60
TOOLSETvc32VER	= 6

#### Borland C++ Builder
TOOLSETbc32	= c:\dev\bcc55
TOOLSETbc32VER	= 55

#### Watcom C/C++
TOOLSETwc32	= c:\dev\watcom
TOOLSETwc32VER	= 11
@


1.1.2.2
log
@Added more config info the build system will need.
@
text
@d4 1
a4 1
#  that needs editing to build Expect.
d7 1
a7 1
# RCS: @@(#) $Id: mkconfig.mif,v 1.1.2.1 2001/09/26 13:25:20 davygrvy Exp $
a19 21

#==============================================
# Where is Tcl ?
#==============================================
TCLROOT		= C:\program Files\Tcl


#==============================================
# Does the above Tcl path point to the install
# or sources ?
#==============================================
ISTCLINSTALL	= 1


#==============================================
# Where should we install to ?  Use $(TCLROOT)
# when ISTCLINSTALL == 1
#==============================================
INSTALLDIR	= $(TCLROOT)


d21 1
a21 1
# What Tcl version are we grabbing ?
d23 1
a23 6
TCLMAJOR	= 8
TCLMINOR	= 4

TCL_VERSION	= $(TCLMAJOR)$(TCLMINOR)
TCL_DOTVERSION	= $(TCLMAJOR).$(TCLMINOR)

d33 1
a33 1
#### Borland C++ Builder Commandline tools
d37 1
a37 1
#### OpenWatcom C/C++
a39 4

#### MinGW
TOOLSETmgw32	= c:\dev\mingw
TOOLSETmgw32VER	= 29535
@


1.1.2.3
log
@Changed the build files to be run from the /win directory instead
of the top-root.  This will help get MS project .dsp files working.
@
text
@d7 1
a7 1
# RCS: @@(#) $Id: mkconfig.mif,v 1.1.2.2 2001/09/28 00:41:50 davygrvy Exp $
d11 1
a11 1
#!error Please set the user defaults in this file before using this build system.
a22 1
# This needs to be the source location.
d24 1
a24 1
TCLROOT		= D:\thread_ws\Tcl
d26 6
a31 4
### We can't set this to 1, so don't try.  Expect needs to know some
### deep internals and it's only available with the source dist.
### Expect CANNOT build from the distribution.
ISTCLINSTALL	= 0
d38 1
a38 1
INSTALLDIR	= C:\program Files\Tcl
@


