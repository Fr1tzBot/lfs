1.1.2.1
log
@New top-root makefile under windows.
@
text
@a0 30
#==============================================================================
#
#   Make information file for the Watcom 32-bit C/C++ tools.
#
#
#   WMAKE specific syntax is allowed in this file.
#
#==============================================================================
# RCS: @@(#) $Id: mkwc32.mif,v 1.1.2.2 2001/09/09 04:20:04 davygrvy Exp $
#==============================================================================

TOOLSET	    = wc32
MAKECMD	    = wmake.exe -h -ms $(__MAKEOPTS__) -f makefile.wc32

SYM	    = .SYMBOLIC

!ifdef __LOADDLL__
!  loaddll $(TOOLSETwc32)\bin\wcc386   wccd386
!  loaddll $(TOOLSETwc32)\bin\wpp386   wppd386
!  loaddll $(TOOLSETwc32)\bin\wlink    wlink
!  loaddll $(TOOLSETwc32)\bin\wlib     wlibd
!endif


.AFTER
	@@echo.
	@@echo Thank you for choosing Tcl.

.ERROR
	@@echo lalala@


